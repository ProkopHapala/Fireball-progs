all :
	make fireball.x

veryclean :  clean
	rm -f fireball.x libfireball.a

extraclean : veryclean

.PHONY :  clean veryclean extraclean

clean :
	rm -f -r core *.o .nfs* rii_files fireball.x.ip*  *.mod ldtmp* *.vo *~ *.il

F90 = gfortran

FFLAGS = -O3 -mtune=native -ftree-vectorize  -ffree-form -ffree-line-length-none -freal-4-real-8 

LFLAGS = -lmkl_scalapack_lp64 -lmkl_blacs_openmpi_lp64 -lmkl_lapack95_lp64 -lmkl_intel_lp64 -lmkl_intel_thread -lmkl_core -liomp5 -lpthread -lfftw3xf_gnu -lm -Bdynamic -lmpi-L/home/prokop/SW/intel//mkl/lib/intel64 -L/home/prokop/SW/intel//mkl/lib/intel64 -I/home/prokop/SW/intel//mkl/include/fftw -L/usr/lib/x86_64-linux-gnu/openmpi/lib -I/usr/lib/x86_64-linux-gnu/openmpi/include 

fireball.x : $(OBJECTS)
	$(F90) -o fireball.x  $(FFLAGS) $(OBJECTS)

