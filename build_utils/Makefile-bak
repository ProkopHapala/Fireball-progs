	$(F90) $(FFLAGS) -c DFTD3/sizes.f90 -o build/sizes.o
pars.o : DFTD3/pars.f90
	$(F90) $(FFLAGS) -c DFTD3/pars.f90 -o build/pars.o
core.o : DFTD3/core.f90
	$(F90) $(FFLAGS) -c DFTD3/core.f90 -o build/core.o
api.o : DFTD3/api.f90
	$(F90) $(FFLAGS) -c DFTD3/api.f90 -o build/api.o
dftd3_corrections.o : DFTD3/dftd3_corrections.f90
	$(F90) $(FFLAGS) -c DFTD3/dftd3_corrections.f90 -o build/dftd3_corrections.o


